{
  "trailingComma": "es5",
  "semi": false,
  "tabWidth": 2,
  "singleQuote": false,
  "arrowParens": "avoid",
  "useTabs": false,
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "importOrder": [
    "^react$",
    "^next",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/app/(.*)$",
    "^@/components/(.*)$",
    "^@/providers/(.*)$",
    "^@/schema/(.*)$",
    "^@/types/(.*)$",
    "",
    "^@/lib/(.*)$",
    "^@/hooks/(.*)$",
    "^@/utils/(.*)$",
    "",
    "^[./]"
  ],
  "importOrderParserPlugins": ["typescript", "jsx"],
  "tailwindAttributes": ["class", "className"],
  "tailwindFunctions": ["clsx", "cn"]
}
